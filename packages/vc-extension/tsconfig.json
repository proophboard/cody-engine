{
  "compilerOptions": {
    "target": "ES2022",
    "module": "ES2020",
    "moduleResolution": "Node",
    "lib": ["ES2022", "WebWorker"],
    "outDir": "dist",
    "strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx",
    "resolveJsonModule": true,
  },
  "include": ["src/**/*"],
  "extends": "../../tsconfig.base.json"
}
